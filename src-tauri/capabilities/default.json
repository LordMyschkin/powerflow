{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main", "popover", "settings"],
  "permissions": [
    "core:default",
    "opener:default",
    "positioner:default",
    "core:window:default",
    "core:window:allow-hide",
    "core:window:allow-start-dragging",
    "shell:allow-open",
    "notification:default",
    "dialog:allow-save"
  ]
}
